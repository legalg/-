#include<stdio.h>
int main(void)
{
	int arr1[3][3] = { {1,2,3},{4,5,6,},{7,8,9} };
	int arr2[3][3] = { {1,2,3},{4,5,6,},{7,8,9} };
	int arr3[3][3] = { 0 };
	//打印arr1
	printf("arr1；\n");
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			printf("%d\t", arr1[i][j]);
		}
		printf("\n");
	}
	printf("\n");
	//打印arr2
	printf("arr2；\n");
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			printf("%d\t", arr2[i][j]);
		}
		printf("\n");
	}
	printf("\n");
	//打印点乘
	printf("矩阵点乘>>arr3；\n");
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			printf("%d\t", arr3[i][j] = arr1[i][j] * arr2[i][j]);
		}
		printf("\n");
	}
	return 0;
}
